<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/ic_action_map.png"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_action_map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ic_action_list.png"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/ic_action_list.png"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_action_list"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ic_action_account.png"
        app:layout_constraintStart_toEndOf="@+id/ic_action_map.png" />

    <ImageView
        android:id="@+id/ic_action_account.png"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_action_account"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ic_action_list.png" />


</androidx.constraintlayout.widget.ConstraintLayout>